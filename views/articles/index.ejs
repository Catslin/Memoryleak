<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/css/homePage.css">
</head>
<body>
    <div class="page-container" onselectstart="return false" oncontextmenu="return false">
        <div class="self-page">
            <div class="icon">
                <img src="./image/icon.png">
                <span>Catslin</span>
            </div>
            <div class="tages">
                <div class="tag">java</div>
                <div class="tag">jvm</div>
                <div class="tag">spring</div>
                <div class="tag">springboot</div>
                <div class="tag">maven</div>
                <div class="tag">gradle</div>
                <div class="tag">intellij</div>
                <div class="tag">eclipse</div>
                <div class="tag">mysql</div>
                <div class="tag">node</div>
            </div>
        </div>
        <div class="center-show">
            <div class="center-nav">
                <input type="text">
                <a href="/articles/new">SEND</a>
            </div>
                <% articles.forEach(article => { %>
                    <a href="articles/<%= article.slug %>">
                        <div class="article-card">
                            <div class="card-top">
                                <img src="./image/3.png" alt="">
                                <span>Super萝莉控</span>
                                <span><%= article.createdAt.toLocaleDateString() %></span>
                            </div>
                            <div class="card-center">
                                <img src="<%= article.image %>" alt="">
                            </div>
                            <div class="card-context">
                                <span><%= article.title %></span>
                                <p><%= article.description %>.</p>
                            </div>
                            <div class="card-buttom">
                                <img src="./image/2 (4).png" alt="">
                                <img src="./image/2 (1).png" alt="">
                                <img src="./image/(2).png" alt="">
                                <img src="./image/2 (2).png" alt="">
                                <img src="./image/2 (3).png" alt="">
                            </div>
                        </div>
                    </a>
                <% }) %>
            </div>
            <div class="footer-nav"></div>
        </div>
    </div>
</body>
</html>